 <nav>{{> logout}}</nav>
<h1>Edit your profile</h1> 
<h3><a href ="/edit">{{error}}</a></h3>


<form class ="edit" method="POST">
   {{#rows}}  
   <input type="text" name="firstName" value = "{{first}}"" autocomplete="off"></input>
 <input type="text" name="lastName" value = "{{last}}" autocomplete="off"></input>
 <input type="email" name="email" value = "{{email}}"" autocomplete="off"></input>
    <input type="password" name="password" placeholder = "change password", autocomplete="off"></input>
  <input type="text" name="age" value = "{{age}}"" autocomplete="off"></input>
 <input type="text" name="city" value = "{{city}}" autocomplete="off"></input>
 <input type="text" name="url" value = "{{url}}"" autocomplete="off"></input> 
 <h3><a href ="/edit">{{error}}</a></h3>
 {{/rows}}  
  <input type="hidden" name="_csrf" value="{{csrfToken}}">
    <div><button type="submit">Edit Profile!</button>
</form>
